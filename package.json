{
  "name": "datasource-solarwinds-swis",
  "version": "0.0.1",
  "description": "DataSource for SolarWinds SWIS HTTP REST endpoint.",
  "scripts": {
    "build": "tsc -p tsconfig.json && npx rollup -c rollup.config.js",
    "dev": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "e2e": "playwright test",
    "server": "docker-compose up --build",
    "sign": "npx @grafana/sign-plugin@latest",
    "watch": "webpack -c ./.config/webpack/webpack.config.ts --env development --watch"
  },
  "author": "Eduard Tichy",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.0",
    "@grafana/ui": "^10.0.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/webpack": "^5.28.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.40.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^6.0.1"
  }
